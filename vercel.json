{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://144.126.215.218:8888/api/:path*"
    },
    {
      "source": "/ws/:path*", 
      "destination": "http://144.126.215.218:8888/ws/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 